{
  "name": "modette/core-ext-orm",
  "description": "ORM for Modette",
  "type": "library",
  "license": "MPL-2.0",
  "keywords": [
    "nextras",
    "orm"
  ],
  "homepage": "https://github.com/modette/modette",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "contributte/nextras-orm-events": "^0.4.0",
    "modette/core": "^1.0.0",
    "nextras/orm": "^3.0.1"
  },
  "require-dev": {
    "modette/core-ext-sql": "^1.0.0",
    "ninjify/nunjuck": "^0.2.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Modette\\Orm\\": "src"
    }
  },
  "scripts": {
    "tests": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "phpstan-install": [
      "mkdir -p temp/phpstan",
      "composer require -d temp/phpstan phpstan/phpstan:^0.10",
      "composer require -d temp/phpstan phpstan/phpstan-deprecation-rules:^0.10",
      "composer require -d temp/phpstan phpstan/phpstan-nette:^0.10",
      "composer require -d temp/phpstan phpstan/phpstan-strict-rules:^0.10",
      "composer require -d temp/phpstan nextras/orm-phpstan:^0.2"
    ],
    "phpstan": [
      "temp/phpstan/vendor/bin/phpstan analyse -l max -c phpstan.neon src"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
