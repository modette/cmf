{
  "name": "modette/core-ext-ui",
  "description": "UI layer for Modette applications",
  "type": "library",
  "license": "MPL-2.0",
  "keywords": [
    "controls",
    "forms",
    "presenters",
    "ui"
  ],
  "homepage": "https://github.com/modette/modette",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "contributte/application": "^0.3.1",
    "contributte/event-dispatcher-extra": "^0.4.1",
    "contributte/forms": "^0.3.0",
    "contributte/phpdoc": "^0.2.0",
    "modette/core": "^1.0.0",
    "modette/core-ext-http": "^1.0.0",
    "modette/core-ext-templates": "^1.0.0",
    "nette/component-model": "^2.4.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.2.1",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-nette": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Modette\\UI\\": "src"
    }
  },
  "scripts": {
    "tests": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "phpstan": [
      "vendor/bin/phpstan analyse -l max -c phpstan.neon src"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
