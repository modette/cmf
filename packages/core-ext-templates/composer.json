{
  "name": "modette/core-ext-templates",
  "description": "Latte templates for Modette",
  "type": "library",
  "license": "MPL-2.0",
  "keywords": [
    "assets",
    "latte",
    "templates",
    "themes",
    "translations"
  ],
  "homepage": "https://github.com/modette/modette",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "contributte/event-dispatcher-extra": "^0.4.1",
    "contributte/latte-parsedown-extra": "^1.3",
    "latte/latte": "~2.4.8",
    "modette/core": "^1.0.0",
    "nette/application": "^2.4.12"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.2.1",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-nette": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Modette\\Templates\\": "src"
    }
  },
  "scripts": {
    "tests": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "phpstan": [
      "vendor/bin/phpstan analyse -l max -c phpstan.neon src"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
