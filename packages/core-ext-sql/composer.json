{
  "name": "modette/core-ext-sql",
  "description": "SQL migrations and DBAL for Modette",
  "type": "library",
  "license": "MPL-2.0",
  "keywords": [
    "dbal",
    "migrations",
    "nextras",
    "sql"
  ],
  "homepage": "https://github.com/modette/modette",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "modette/core": "^1.0.0",
    "nextras/dbal": "^3.0.0",
    "nextras/migrations": "^3.0.5"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.2.1",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-nette": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Modette\\Sql\\": "src"
    }
  },
  "scripts": {
    "tests": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "phpstan": [
      "vendor/bin/phpstan analyse -l max -c phpstan.neon src"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
