{
	"name": "modette/core-ext-sql",
	"description": "SQL migrations and DBAL for Modette",
	"type": "library",
	"license": "MPL-2.0",
	"keywords": [
		"dbal",
		"migrations",
		"nextras",
		"sql"
	],
	"homepage": "https://github.com/modette/modette",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://marek-bartos.cz"
		}
	],
	"require": {
		"php": "^7.2.0",
		"modette/core": "^1.0.0",
		"nextras/dbal": "~3.2.0",
		"nextras/migrations": "~3.1.0"
	},
	"require-dev": {
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-shim": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modette\\Sql\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\Sql\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
