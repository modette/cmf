{
  "name": "modette/generator",
  "description": "Generate your Modette project with simple console commands",
  "type": "project",
  "license": "MPL-2.0",
  "keywords": [
    "cli",
    "console",
    "generator"
  ],
  "homepage": "https://github.com/modette/modette",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.2.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Modette\\Generator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Modette\\Generator\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
