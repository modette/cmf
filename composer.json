{
	"replace": {
		"modette/core": "self.version",
		"modette/core-ext-admin": "self.version",
		"modette/core-ext-api": "self.version",
		"modette/core-ext-front": "self.version",
		"modette/core-ext-http": "self.version",
		"modette/core-ext-mail": "self.version",
		"modette/core-ext-orm": "self.version",
		"modette/core-ext-sql": "self.version",
		"modette/core-ext-templates": "self.version",
		"modette/core-ext-ui": "self.version",
		"modette/generator": "self.version",
		"modette/module-installer": "self.version",
		"modette/project": "self.version",
		"modette/code-generator": "self.version"
	},
	"type": "project",
	"description": "Modette root package used for development",
	"homepage": "https://github.com/modette/modette",
	"license": "MPL-2.0",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://marek-bartos.cz"
		}
	],
	"require": {
		"php": "^7.2.0",
		"composer-plugin-api": "^1.0.0",
		"apitte/console": "^0.6.0",
		"apitte/core": "^0.6.0",
		"apitte/debug": "^0.6.0",
		"apitte/middlewares": "^0.6.0",
		"apitte/negotiation": "^0.6.0",
		"apitte/openapi": "^0.6.0",
		"contributte/application": "^0.4.0",
		"contributte/cache": "^0.4.0",
		"contributte/console": "^0.7.0",
		"contributte/console-extra": "^0.5.0",
		"contributte/di": "^0.4.0",
		"contributte/event-dispatcher": "^0.6.0",
		"contributte/event-dispatcher-extra": "^0.6.0",
		"contributte/flysystem": "^0.2.0",
		"contributte/forms": "^0.4.0",
		"contributte/mail": "^0.4.0",
		"contributte/middlewares": "^0.9.0",
		"contributte/model": "^0.2.0",
		"contributte/monolog": "^0.3.0",
		"contributte/nextras-orm-events": "^0.5.0",
		"contributte/phpdoc": "^0.3.0",
		"contributte/psr7-http-message": "^0.6.0",
		"contributte/tracy": "^0.4.0",
		"latte/latte": "~2.5.1",
		"marc-mabe/php-enum": "^4.0.0",
		"modette/exceptions": "^1.0.0",
		"moontoast/math": "^1.1.0",
		"nette/application": "~3.0.0",
		"nette/component-model": "^3.0.0",
		"nette/http": "~3.0.2",
		"nette/neon": "~3.0.0",
		"nette/schema": "~1.0.0",
		"nette/utils": "~3.0.0",
		"nextras/dbal": "~3.1.0",
		"nextras/migrations": "~3.1.0",
		"nextras/orm": "~3.1.0",
		"ramsey/uuid": "^3.8.0"
	},
	"require-dev": {
		"composer/composer": "^1.7.0",
		"contributte/neonizer": "^0.4.0",
		"nextras/orm-phpstan": "^0.3.0",
		"ninjify/qa": "^0.9.0",
		"pepakriz/phpstan-exception-rules": "^0.8.1",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-phpunit": "^0.11.2",
		"phpstan/phpstan-shim": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3",
		"symplify/monorepo-builder": "^6.0.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "packages/project/src",
			"Boot\\": "packages/project/boot",
			"Modette\\Admin\\": "packages/core-ext-admin/src",
			"Modette\\Api\\": "packages/core-ext-api/src",
			"Modette\\CodeGenerator\\": "packages/code-generator/src",
			"Modette\\Core\\": "packages/core/src",
			"Modette\\Front\\": "packages/core-ext-front/src",
			"Modette\\Http\\": "packages/core-ext-http/src",
			"Modette\\Mail\\": "packages/core-ext-mail/src",
			"Modette\\ModuleInstaller\\": "packages/module-installer/src",
			"Modette\\Orm\\": "packages/core-ext-orm/src",
			"Modette\\Sql\\": "packages/core-ext-sql/src",
			"Modette\\Templates\\": "packages/core-ext-templates/src",
			"Modette\\UI\\": "packages/core-ext-ui/src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\App\\": "packages/project/tests",
			"Tests\\Modette\\Admin\\": "packages/core-ext-admin/tests",
			"Tests\\Modette\\Api\\": "packages/core-ext-api/tests",
			"Tests\\Modette\\CodeGenerator\\": "packages/code-generator/tests",
			"Tests\\Modette\\Core\\": "packages/core/tests",
			"Tests\\Modette\\Front\\": "packages/core-ext-front/tests",
			"Tests\\Modette\\Http\\": "packages/core-ext-http/tests",
			"Tests\\Modette\\Mail\\": "packages/core-ext-mail/tests",
			"Tests\\Modette\\ModuleInstaller\\": "packages/module-installer/tests",
			"Tests\\Modette\\Orm\\": "packages/core-ext-orm/tests",
			"Tests\\Modette\\Sql\\": "packages/core-ext-sql/tests",
			"Tests\\Modette\\Templates\\": "packages/core-ext-templates/tests",
			"Tests\\Modette\\UI\\": "packages/core-ext-ui/tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
